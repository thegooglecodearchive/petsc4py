# -*- makefile -*-

CFLAGS	 = -O3
CPPFLAGS = 
F90FLAGS =-Wall -O3 -funroll-loops -march=pentium4 -msse2 -fomit-frame-pointer
LOCDIR   = 


ALL: poisson3d

poisson3d: poisson3d.o
	mpif90 ${F90FLAGS} -c -o del2lib.o del2lib.f90
	-${CLINKER} -o $@ $< del2lib.o ${PETSC_KSP_LIB}
	${RM} poisson3d.o del2lib.o

lclean:
	${RM} poisson3d solvesys


## PETSC_DIR=/usr/local/petsc/2.4.0
## PETSC_ARCH=linux-gnu
include ${PETSC_DIR}/conf/base
